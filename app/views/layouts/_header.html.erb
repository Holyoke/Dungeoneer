<header class='navigation' role='banner'>
  <div class='navigation-wrapper'>
    <div class='logo'><%= link_to 'JobWalk' %></div>
    <a href='javacript:void(0)' class='navigation-menu-botton' id='js-mobile-meny'>Menu</a>
    <nav role='navigation'>
      <ul id='js-navigation-meny' class='navigation-menu show'>
        <% if user_signed_in? %>
          <li class='nav-link'><%= link_to 'Users', users_path %>
          <li class='nav-link'><%= link_to 'Log out', destroy_user_session_path, method: 'delete'%></li>
          <% else %>
            <li class='nav-link'><%= link_to 'Sign in', new_user_session_path %>
            <li class='nav-link'><%= link_to 'Sign up', new_user_registration_path %></li>
          <% end %>
      </ul>
    </nav>
  </div>
</header>
